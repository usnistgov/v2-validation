						<NumberList Path="1[1]" CSV=" 1 , 2.0 , 3 "/>
					</Assertion>
				</Constraint>
				
				<Constraint ID="String List">
					<Description>String List</Description>
					<Assertion> 
						<StringList Path="1[1]" CSV="1,2"/>
