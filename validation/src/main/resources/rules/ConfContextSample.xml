					<Description>Simple Value</Description>
					<Assertion>
						<SimpleValue Path="1[1]" Operator="NE" Value="XX" Type="Number"/>
					</Assertion>
				</Constraint>
				
				<!-- Combination Expressions -->
				
				<Constraint ID="AND">
					<Description>AND</Description>
					<Assertion>
						<AND>
							<Presence Path="1[1]"/>
							<Presence Path="2[2]"/>
						</AND>
					</Assertion>
				</Constraint>
				
				<Constraint ID="OR">
					<Description>OR</Description>
					<Assertion>
						<OR>
							<Presence Path="1[1]"/>
							<Presence Path="2[2]"/>
						</OR>
					</Assertion>
				</Constraint>
				
				<Constraint ID="NOT">
					<Description>NOT</Description>
					<Assertion>
						<NOT>
							<Presence Path="1[1]"/>
						</NOT>
					</Assertion>
				</Constraint>
				
				
				<Constraint ID="NOT">
					<Description>NOT - Constraint</Description>
					<Assertion>
						<NOT>
							<AND>
								<Presence Path="1[1]"/>
